package myPackage;

class Hello {
    static class Thing {
        public int size;

        Thing() {
            size = 100;
            System.out.println(""+size);
        }
    }

    public static void main(String[] args) {
     //   Hello hello1 = new Hello();
    	Thing thing = new Thing();
        System.out.println("Hello, World!");
    }
}
