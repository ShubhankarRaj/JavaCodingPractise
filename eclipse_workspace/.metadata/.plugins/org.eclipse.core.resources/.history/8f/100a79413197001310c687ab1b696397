package SCJPTestSat;

class Vehicle1 {
    public void printSound() {
        System.out.print("vehicle");
    }
}

class Car extends Vehicle1 {
    public void printSound() {
        System.out.print("car");
    }
}

class Bike extends Vehicle1 {
    public void printSound() {
        System.out.print("bike");
    }
}

public class Vehicle {
    public static void main(String[] args) {
        Vehicle1 v = new Car();
//        Bike b = (Bike) v;
        
        v.printSound();
//        b.printSound();
    }   
}
